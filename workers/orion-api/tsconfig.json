{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"],
    "noEmit": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "baseUrl": "src"
  },
  "include": ["src/**/*.ts"]
}